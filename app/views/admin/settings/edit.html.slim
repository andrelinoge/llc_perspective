- breadcrumbs([{text: 'Site settings', url: admin_settings_path}, {text: 'Edit setting'}])


= simple_form_for(@setting, url: admin_setting_path(@setting.var), method: :patch) do |f| 
  = f.input :value, label: @setting.var, value: @setting.value.presence
  = f.submit value: 'Save', class: 'btn btn-secondary'
